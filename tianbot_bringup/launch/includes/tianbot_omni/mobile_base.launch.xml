<?xml version="1.0"?>

<launch>
  <arg name="serial_port" />
  <arg name="robot_name" />
  <node pkg="tianboard" type="tianboard_node" name="tianboard" output="screen">
    <param name="serial_port" type="string" value="$(arg serial_port)"/>
    <remap from="tianboard/imu" to="$(arg robot_name)/imu" />
    <remap from="tianboard/odom" to="$(arg robot_name)/odom" />
    <remap from="tianboard/cmd_vel" to="$(arg robot_name)/cmd_vel" />
    <remap from="tianboard/uwb" to="$(arg robot_name)/uwb" /> 
  </node>
</launch>
